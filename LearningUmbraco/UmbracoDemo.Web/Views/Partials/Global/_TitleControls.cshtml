@inherits UmbracoTemplatePage
@{
    var validTitle = Umbraco.AssignedContentItem.HasProperty("title") && Umbraco.AssignedContentItem.GetPropertyValue("title") != null;
    var validsubTitle = Umbraco.AssignedContentItem.HasProperty("subTitle") && Umbraco.AssignedContentItem.GetPropertyValue("subTitle") != null;
    var title = validTitle ? Umbraco.AssignedContentItem.GetPropertyValue("title").ToString() : Umbraco.AssignedContentItem.Name;
    var subTitle = validsubTitle ? Umbraco.AssignedContentItem.GetPropertyValue("subTitle").ToString() : Umbraco.AssignedContentItem.Name;
}

<div class="row">
    <div class="col-md-6 col-md-offset-3 text-center fh5co-heading">
        <h2>@title</h2>
        @Html.Raw(subTitle)
    </div>
</div>